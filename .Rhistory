students
top <- students %>%
slice(1:nrow(students)-1) %>%
mutate(tempcol1=ifelse(is.na(FURTH_STUDIES), Q11_7,FURTH_STUDIES )) %>%
mutate(FURTH_STUDIES=ifelse(is.na(tempcol1), Q52_6,tempcol1))
#select(1:70, 73)
