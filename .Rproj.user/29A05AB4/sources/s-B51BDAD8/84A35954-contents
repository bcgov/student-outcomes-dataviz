data.frame("Skills Development" = c("Read & comprehend material", "Write clearly & concisely","Speak effectively", "Work effectively with others",
                                    "Analyse & think critically", "Resolve issues or problems","Learn independently"), "Values" = (2,3,4,5,6,7,8))

data.frame("skill"=c("a","b"), values=(1,2))

Skills_Development<- c("Read and comprehend material", "Write clearly and concisely","Speak effectively", "Work effectively with others","Analyse and think critically","Resolve issues or problems","Learn independently")
values <- c(1,2,3,4,5,6,7)

data.frame(Skills_Development, values)
